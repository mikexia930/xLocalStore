{
  "name": "XLocalStore",
  "description": "Custom store, Use IndexedDB first, if the browser do not support it, change to Localstorage",
  "keywords": [
    "IndexedDB",
    "Localstorage",
    "浏览器存储"
  ],
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "files": [
    "lib/*"
  ],
  "main": "./lib/xlocalstore.umd.js",
  "module": "./lib/xlocalstore.es.js",
  "exports": {
    ".": {
      "import": "./lib/xlocalstore.es.js",
      "require": "./lib/xlocalstore.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build:web": "LIB_NAME=web vite build",
    "build:lib": "LIB_NAME=lib vite build",
    "build:all": "npm run build:web && npm run build:lib"
  },
  "devDependencies": {
    "vue": "^3.2.45",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "ant-design-vue": "^3.2.16",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
